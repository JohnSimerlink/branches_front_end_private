{
  "name": "branches_front_end_private",
  "version": "1.1.0",
  "scripts": {
    "pre-dev": "time docker build --file=./Dockerfile --tag=branches_front_end ./",
    "dev": "cross-env app_env=development docker run --name=branches_front_end_$(date '+%Y_%m_%d_%H_%M_%S') -it -p 8080:8080 -v \"$(pwd)\":/frontend branches_front_end",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "cross-env NODE_ENV=test ava-ts *.spec.ts app/*.spec.ts app/components/**/*.spec.ts app/core/*.spec.ts app/loaders/**/*.spec.ts app/objects/**/*.spec.ts app/objects/**/**/*.spec.ts app/objects/*.spec.ts --verbose --color",
    "test-some": "cross-env NODE_ENV=test ava-ts --color",
    "test:watch": "cross-env NODE_ENV=test ava-ts *.spec.ts app/*.spec.ts app/components/**/*.spec.ts app/core/*.spec.ts app/loaders/**/*.spec.ts app/objects/**/*.spec.ts app/objects/**/**/*.spec.ts app/objects/*.spec.ts --verbose --color --watch",
    "coverage": "gulp coverage",
    "deploy": "sh .deploy.sh",
    "publish-coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "firebase": "^4.8.0",
    "heap": "^0.2.6",
    "inversify": "^4.11.1",
    "object.entries": "^1.0.4",
    "reflect-metadata": "^0.1.10",
    "vue": "^2.3.4",
    "vue-async-computed": "^3.3.1",
    "vue-stripe": "^0.1.52",
    "vuex": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@types/heap": "^0.2.28",
    "@types/node": "^8.0.50",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "ava": "^0.24.0",
    "ava-ts": "^0.23.0",
    "ava-webpack": "^1.1.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "8.0.4",
    "chai": "^4.1.0",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.4.1",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.5.1",
    "ignore-styles": "^5.0.1",
    "firebase-mock": "^1.1.13",
    "jsdom": "^11.2.0",
    "jsdom-global": "^3.0.2",
    "json-stable-stringify": "^1.0.1",
    "less": "^2.7.2",
    "lesshint": "~5.1.0",
    "lesshint-webpack-plugin": "^0.0.4",
    "less-loader": "^4.1.0",
    "moment": "^2.18.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.18.2",
    "sinon": "^4.1.2",
    "typescript": "^2.6.2",
    "ts-loader": "^5.2.1",
    "ts-node": "^3.3.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "vue-loader": "^12.2.1",
    "vue-router": "^2.7.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-stream": "^4.0.0"
  }
}
