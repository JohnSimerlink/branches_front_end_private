{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": false,
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "arrow-parens" : ["error", "as-needed"],
        "trailing-comma": false,
        "cyclomatic-complexity": [true, 10]
    },
    "rulesDirectory": [],
    "force": true
}